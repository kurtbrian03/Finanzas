{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Pinpon: Unblock Installer and Setup Agent",
      "type": "shell",
      "command": "powershell.exe",
      "args": [
        "-NoProfile",
        "-ExecutionPolicy",
        "Bypass",
        "-Command",
        "& { Start-Process pwsh -ArgumentList '-NoProfile -ExecutionPolicy Bypass -File \"${workspaceFolder}\\\\scripts\\\\unblock_and_setup_agent.ps1\"' -Verb RunAs }"
      ],
      "presentation": {
        "echo": true,
        "reveal": "always",
        "panel": "shared"
      },
      "problemMatcher": []
    }
  ]
}
